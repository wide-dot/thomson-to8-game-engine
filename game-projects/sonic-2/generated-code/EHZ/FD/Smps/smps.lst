                      (./Engine/Sound/Sm):00054                                                                       ;         "playback control"; bits 
                      (./Engine/Sound/Sm):00055                                                                       ;         1 (02h)  seems to be "track is at rest"
                      (./Engine/Sound/Sm):00056                                                                       ;         2 (04h)  SFX is overriding this track
                      (./Engine/Sound/Sm):00057                                                                       ;         3 (08h)  modulation on
                      (./Engine/Sound/Sm):00058                                                                       ;         4 (10h)  do not attack next note
                      (./Engine/Sound/Sm):00059                                                                       ;         7 (80h)  track is playing
0000s                 (./Engine/Sound/Sm):00060                 PlaybackControl                rmb   1
                      (./Engine/Sound/Sm):00061                                                                       ;         "voice control"; bits 
                      (./Engine/Sound/Sm):00062                                                                       ;         0-3 (00h-0Fh) Channel number
                      (./Engine/Sound/Sm):00063                                                                       ;         7   (80h) PSG Track
                      (./Engine/Sound/Sm):00064                                                                       ;         PSG    Chn    |a| |00000|
                      (./Engine/Sound/Sm):00065                                                                       ;         Voice1 0x80 = 100  00000
                      (./Engine/Sound/Sm):00066                                                                       ;         Voice2 0xa0 = 101  00000
                      (./Engine/Sound/Sm):00067                                                                       ;         Voice3 0xc0 = 110  00000                                                      
                      (./Engine/Sound/Sm):00068                                                                       ;         Voice4 0xe0 = 111  00000                                                      
0001s                 (./Engine/Sound/Sm):00069                 VoiceControl                   rmb   1
                      (./Engine/Sound/Sm):00070                                                                       ;         "note control"; bits
                      (./Engine/Sound/Sm):00071                                                                       ;         0-3 (00h-0Fh) Current Block(0-2) and FNum(8)
                      (./Engine/Sound/Sm):00072                                                                       ;         4   (10h) Key On
                      (./Engine/Sound/Sm):00073                                                                       ;         5   (20h) Sustain On
0002s                 (./Engine/Sound/Sm):00074                 NoteControl                    rmb   1
0003s                 (./Engine/Sound/Sm):00075                 TempoDivider                   rmb   1                ; timing divisor; 1 = Normal, 2 = Half, 3 = Third...
0004s                 (./Engine/Sound/Sm):00076                 DataPointer                    rmb   2                ; Track's position
0006s                 (./Engine/Sound/Sm):00077                 Transpose                      rmb   1                ; Transpose (from coord flag E9)
0007s                 (./Engine/Sound/Sm):00078                 Volume                         rmb   1                ; Attenuation - (Dependency) Should follow Transpose
0008s                 (./Engine/Sound/Sm):00079                 VoiceIndex                     rmb   1                ; Current voice in use OR current PSG tone
0009s                 (./Engine/Sound/Sm):00080                 VolFlutter                     rmb   1                ; PSG flutter (dynamically effects PSG volume for decay effects)
000As                 (./Engine/Sound/Sm):00081                 StackPointer                   rmb   1                ; "Gosub" stack position offset (starts at 2Ah, i.e. end of track, and each jump decrements by 2)
000Bs                 (./Engine/Sound/Sm):00082                 DurationTimeout                rmb   1                ; current duration timeout; counting down to zero
000Cs                 (./Engine/Sound/Sm):00083                 SavedDuration                  rmb   1                ; last set duration (if a note follows a note, this is reapplied to 0Bh)
                      (./Engine/Sound/Sm):00084                                                                       ; 0Dh / 0Eh change a little depending on track -- essentially they hold data relevant to the next note to play
000Ds                 (./Engine/Sound/Sm):00085                 NextData                       rmb   2                ; DAC Next drum to play - FM/PSG  frequency
000Fs                 (./Engine/Sound/Sm):00086                 NoteFillTimeout                rmb   1                ; Currently set note fill; counts down to zero and then cuts off note
0010s                 (./Engine/Sound/Sm):00087                 NoteFillMaster                 rmb   1                ; Reset value for current note fill
0011s                 (./Engine/Sound/Sm):00088                 ModulationPtr                  rmb   2                ; address of current modulation setting
0013s                 (./Engine/Sound/Sm):00089                 ModulationWait                 rmb   1                ; Wait for ww period of time before modulation starts
0014s                 (./Engine/Sound/Sm):00090                 ModulationSpeed                rmb   1                ; Modulation Speed
0015s                 (./Engine/Sound/Sm):00091                 ModulationDelta                rmb   1                ; Modulation change per Mod. Step
0016s                 (./Engine/Sound/Sm):00092                 ModulationSteps                rmb   1                ; Number of steps in modulation (divided by 2)
0017s                 (./Engine/Sound/Sm):00093                 ModulationVal                  rmb   2                ; Current modulation value
0019s                 (./Engine/Sound/Sm):00094                 Detune                         rmb   1                ; Set by detune coord flag E1; used to add directly to FM/PSG frequency
001As                 (./Engine/Sound/Sm):00095                 VolTLMask                      rmb   1                ; zVolTLMaskTbl value set during voice setting (value based on algorithm indexing zGain table)
001Bs                 (./Engine/Sound/Sm):00096                 PSGNoise                       rmb   1                ; PSG noise setting
001Cs                 (./Engine/Sound/Sm):00097                 TLPtr                          rmb   2                ; where TL bytes of current voice begin (set during voice setting)
001Es                 (./Engine/Sound/Sm):00098                 InstrTranspose                 rmb   1                ; instrument transpose
                      (./Engine/Sound/Sm):00099                                                                       ;         "InstrAndVolume"; bits
                      (./Engine/Sound/Sm):00100                                                                       ;          FM Instr.  Attnenuation
                      (./Engine/Sound/Sm):00101                                                                       ; FM       0000       xxxx
                      (./Engine/Sound/Sm):00102                                                                       ; FM       0001       xxxx 
                      (./Engine/Sound/Sm):00103                                                                       ; ...  
                      (./Engine/Sound/Sm):00104                                                                       ; PSG      Chn    |a| |1Fh|
                      (./Engine/Sound/Sm):00105                                                                       ; VOL1     0x90 = 100 1xxxx        vol 4b xxxx = attenuation value
                      (./Engine/Sound/Sm):00106                                                                       ; VOL2     0xb0 = 101 1xxxx        vol 4b
                      (./Engine/Sound/Sm):00107                                                                       ; VOL3     0xd0 = 110 1xxxx        vol 4b
                      (./Engine/Sound/Sm):00108                                                                       ; VOL4     0xf0 = 111 1xxxx        vol 4b       
001Fs                 (./Engine/Sound/Sm):00109                 InstrAndVolume                 rmb   1                ; current instrument and volume
0020s                 (./Engine/Sound/Sm):00110                 LoopCounters                   rmb   $A               ; Loop counter index 0
                      (./Engine/Sound/Sm):00111                                                                       ;   ... open ...
                      (./Engine/Sound/Sm):00112                                                                       ; start of next track, every two bytes below this is a coord flag "gosub" (F8h) return stack
                      (./Engine/Sound/Sm):00113                                                                       ;
                      (./Engine/Sound/Sm):00114                                                                       ;        The bytes between +20h and +29h are "open"; starting at +20h and going up are possible loop counters
                      (./Engine/Sound/Sm):00115                                                                       ;        (for coord flag F7) while +2Ah going down (never AT 2Ah though) are stacked return addresses going
                      (./Engine/Sound/Sm):00116                                                                       ;        down after calling coord flag F8h.  Of course, this does mean collisions are possible with either
                      (./Engine/Sound/Sm):00117                                                                       ;        or other track memory if you're not careful with these!  No range checking is performed!
                      (./Engine/Sound/Sm):00118                                                                       ;
                      (./Engine/Sound/Sm):00119                                                                       ;        All tracks are 2Ah bytes long
